{
  "name": "doctor-octopus",
  "version": "1.0.0",
  "description": "doctor-octopus app",
  "main": "setup.sh",
  "type": "module",
  "scripts": {
    "client": "cd client && npm run dev",
    "fixme": "cd server && bash start.sh fixme false",
    "format": "prettier --write .",
    "healthcheck": "curl --fail http://localhost:8000/health || exit 1",
    "lint": "npx eslint .",
    "log": "bash utils/log.sh",
    "notification": "cd server && bash start.sh notification",
    "prompt":"bash utils/prompt.sh",
    "pull": "git stash && git checkout main && git pull",
    "restart": "npm run stop && npm run start",
    "restart:prod": "npm run stop && npm run start:prod",
    "restart:server:prod": "npm run stop && npm run start:server:prod",
    "restart:notification": "npm run stop:notification && npm run notification",
    "restart:notification:prod": "bash utils/restart-service.sh notification",
    "server": "cd server && bash start.sh main true",
    "server:noinit": "cd server && bash start.sh main false",
    "start": "concurrently \"cd client && npm run dev\" \"cd server && bash start.sh main true\"",
    "start:prod": "bash utils/start.sh",
    "start:fixme": "bash utils/start.sh fixme",
    "start:fixme:prod": "bash utils/restart-service.sh fixme",
    "start:server": "bash utils/start.sh server",
    "start:server:prod": "bash utils/restart-service.sh server",
    "setup": "bash utils/setup-app.sh",
    "setup:lite": "bash utils/setup-app.sh lite",
    "setup:all": "bash utils/setup-app.sh all",
    "stop": "bash utils/stop.sh",
    "stop:fixme": "bash utils/stop.sh fixme",
    "stop:server": "bash utils/stop.sh server",
    "stop:notification": "bash utils/stop.sh notification",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isahil/doctor-octopus.git"
  },
  "author": "Imran Sahil",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isahil/doctor-octopus/issues"
  },
  "homepage": "https://github.com/isahil/doctor-octopus#readme",
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "eslint": "^9.30.0",
    "prettier": "^3.6.2"
  }
}
