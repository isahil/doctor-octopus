{
  "name": "doctor-octopus",
  "version": "1.0.0",
  "description": "doctor-octopus app",
  "main": "setup.sh",
  "type": "module",
  "scripts": {
    "fix": "cd server && python src/component/fix.py",
    "format": "prettier --write .",
    "lint": "npx eslint .",
    "notification": "cd server && bash start.sh notification",
    "start": "concurrently \"cd client && npm run dev\" \"cd server && bash start.sh main\"",
    "start-prod": "concurrently \"cd client && npm run dev\" \"cd server && bash start.sh main\"",
    "setup": "sh utils/setup-app.sh",
    "setup-lite": "sh utils/setup-app.sh lite",
    "setup-all": "sh utils/setup-app.sh all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isahil/doctor-octopus.git"
  },
  "author": "Imran Sahil",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/isahil/doctor-octopus/issues"
  },
  "homepage": "https://github.com/isahil/doctor-octopus#readme",
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "eslint": "^9.19.0",
    "prettier": "^3.4.2"
  }
}
